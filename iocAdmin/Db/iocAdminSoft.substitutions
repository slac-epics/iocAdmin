file ioc.template
{
pattern { IOCNAME , TODFORMAT           }
	{ $(IOC)  , "%m/%d/%Y %H:%M:%S" }
}
file iocGeneralTime.template
{
pattern { IOCNAME }
	{ $(IOC)  }
}
file iocAdminScanMon.db
{
	pattern
	{	IOCNAME	 }
	{	"$(IOC)" }
}
file epicsEnvVars.db
{
	{ IOCNAME = "\$(IOC)" }
}
